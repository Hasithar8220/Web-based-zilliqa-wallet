  <body>


      <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
          <div class="container">
            <a class="navbar-brand" href="/"><h1>Web based blockchain Wallet ( With Zillqa )</h1></a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup"
              aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
              <span class="navbar-toggler-icon"></span>
            </button>
            
          </div>
        </nav>

    <div class="container">

  <h4>Select Key Store File To Get Started!</h4>


  <fieldset>

      

      <form id="frmUploader" enctype="multipart/form-data" action="/authWallet" method="post">
        <input type="file" name="keyUploader" multiple /> 
        <input name="passPhrase" type="password" class="ghost-input" placeholder="Pass Phrase" required>
        <input type="submit" name="submit" id="btnSubmit" value="Restore Wallet" /> </form>
      
    </fieldset>

    </div>
  </body>
